#!/Users/hyojinyi/Desktop/practice/bloomsTag/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'bloomsTag','console_scripts','blooms'
__requires__ = 'bloomsTag'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('bloomsTag', 'console_scripts', 'blooms')()
    )
